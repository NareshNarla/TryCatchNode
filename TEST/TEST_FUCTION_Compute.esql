               
DECLARE ASD EXTERNAL CHARACTER;
CREATE COMPUTE MODULE TEST_FUCTION_Compute
	
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE abc,ASD,ZXC CHARACTER;
		DECLARE id CHARACTER;
		SET id = InputRoot.XMLNSC.GETDETAIL.ID;
		SET Environment.abc = GETDETAILS(id);
		SET ASD = Environment.abc;
		SET ZXC = '234';
	END;
	CREATE FUNCTION GETDETAILS (IN DETAILS CHARACTER ) RETURNS CHARACTER
	BEGIN
		DECLARE XYZ CHARACTER;
		IF DETAILS  = '12' THEN
			SET XYZ = 'AVINASH' ;
		END IF;
		RETURN XYZ;
	END;
END MODULE;
